#include <stdio.h>
int maxi(int a, int b, int c) {
    if (a >= b && a >= c) {
        return a;
    } 
    else if (b >= a && b >= c) {
        return b;
    } 
    else {
        return c;
    }
}

int main() {
    int val1 = 7;  
    int val2 = 5;  
    int val3 = 25;  

    int max = maxi(val1, val2, val3);

    printf("The maximum value is: %d\n", max);

    return 0;
}
